{
    "name": "MV3 Migration - content script example",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "serviceWorker.js"
    },
    "permissions": ["scripting", "activeTab"],
    "action": {
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "js": ["contentScript.js"],
            "matches": ["https://github.com/*"]
        }
    ]
}
